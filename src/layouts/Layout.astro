---
import Head from './Head.astro';
import Header from '../components/Header.astro';
export interface Props {
  title: string;
  page: string;
}

const { title, page = "" } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <Head title={title} page={page} />
  <body>
    <Header />
    <slot />
  </body>
</html>
<style is:global></style>
